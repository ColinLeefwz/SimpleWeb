#content
  - @sessions.each do |session|
    .item
      .thumbnail
        = image_tag(session.image_url)
      .caption
        .title
          = session.title
        %p Expert: #{session.expert.name}
        %p Description:
        %p= session.description
        %p Status: #{session.status}
        = social_share_button_tag("asd")
        = render 'linked_in_share_button'
