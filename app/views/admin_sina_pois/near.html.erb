<table border="1">
  <tr>
    <th>id</th>
    <th>名称</th>
    <th>电话</th>
    <th>城市代码</th>
    <th>地址</th>
    <th>坐标</th>
    <th>类型</th>

  </tr>

  <% @shops.each do |shop| %>
    <tr>
      <td><%= shop.id.to_i %></td>
      <td><%= shop.name %></td>
      <td><%= shop.phone %></td>
      <td><%= shop.city %></td>
      <td><%= shop.addr %></td>
      <td><%= shop.lob if shop.respond_to?(:lob) %></td>
      <td><%= shop.type %></td>
    </tr>

  <% end %>
</table>


