			});	
		}else if(runing=="checkboxs2"){
			AllNoDH();
		}
		
		var timer;
		$("#Question1, #Question2").click(function(){
			var relnum=$(this).attr("rel");
			$("#Answer"+relnum).slideToggle();
			timer=setTimeout(function(){
				$("#Answer"+relnum).slideUp();
			},10000);
		});
		
		$("#Answer1,#Answer2").mouseover(function(){
			clearTimeout(timer);
		}).mouseout(function(){
			var obj=$(this);
			if($(this).css("display")=="none"){return false;}
			timer=setTimeout(function(){
				obj.slideUp();
			},2000);
		});
	});
	</script>
