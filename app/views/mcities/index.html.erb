

<h1>城市列表</h1>
<%= link_to '添加城市', new_mcity_path %>
<% form_tag shops_path,:method =>:get do %>
  <p>
   
  </p>
<% end %>
<table border="1">
  <tr>
  <th>Id</th>
    <th>城市名称</th>
    <th>英文名称</th>   
    <th>预览</th>
    <th>编辑</th>

  </tr>

<% for city in @mcities %>
  <tr>
    <td><%=h city.id   %></td>
    <td><%=h city.name   %></td>
     <td><%=h city.eng_name   %></td>
     <td><%=link_to "预览", mcity_path(city)   %></td>
     <td><%=link_to "编辑", edit_mcity_path(city)   %></td>
  </tr>
<% end %>
</table>
<hr />

<div id="paginate" style="text-align: center">
  <%= generate_paginate @mcities %>
</div>
