<div id="container">

<% @users.each do |user| %>
   <div class="box photo col3">
    <a id="imgt2" href="<%= UserLogo.img_url(user.head_logo_id,:t2) %>" title="<%= user.head_logo_id %>"><%= image_tag(UserLogo.img_url(user.head_logo_id,:t1), :size => "120x120") if user.head_logo_id%></a>
  </div>
<% end %>
  
</div>

<%= generate_paginate3.html_safe %>

<script type="text/javascript" src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.6.2.min.js"></script> 
<%= javascript_include_tag "fancybox/jquery.fancybox-1.3.4.pack"%>
<%= javascript_include_tag "fancybox/jquery.mousewheel-3.0.4.pack"%>
<%= javascript_include_tag "jquery.masonry.min.js"%>

<%= stylesheet_link_tag "jquery.fancybox-1.3.4"%>
<script type="text/javascript">
  $(document).ready(function() {
      $("a#imgt2").fancybox({
        'titlePosition': 'inside'
      });
  });

  $(function(){

    var $container = $('#container');
  
    $container.imagesLoaded( function(){
      $container.masonry({
        itemSelector : '.box'
      });
    });
  
  });
</script>