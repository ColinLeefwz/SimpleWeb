.dash-wrapper
  .container-fluid
    .row-fluid
      .span3.side-menu
        = render 'side_menu'
      .span9.main-menu
        = simple_form_for @email_message, url: refer_new_expert_experts_path do |f|
          %h3 Refer a new Expert
          = f.input :subject, label: "Subject:"
          = f.input :to, label: "To:"
          = f.button :submit
