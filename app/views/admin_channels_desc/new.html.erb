<h2>添加渠道</h2>
<br>
<%= form_for(@channel, {:url => '/admin_channels_desc/create'}) do |f| %>

  <p>
    渠道号：<br />
    <%= f.text_field :num, :size=>20%>
  </p>
  <p>
    使用者：<br />
    <%= f.text_field :who, :size=>20 %>
  </p>
  <p>
    用途：<br />
    <%= f.text_area :where, :rows => 6 %>
  </p>
  <p>
    <%= f.check_box :type %> 是否有中间页
  </p>

  <p>
    <%= f.submit "保存" %>
  </p>
<% end %>