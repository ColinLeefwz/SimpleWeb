<h1>每日新增用户城市统计</h1>

<table>
  <tr>
    <th>时间</th>
    <th>统计结果</th>
  </tr>
  <% @user_city_days.each do |user_city_day| %>
    <tr>
      <td>
        <%= user_city_day._id   %>
      </td>
      <td>
        <%= user_city_day.datas.map{|k,v| "#{k}:#{v.to_i}"}.join(';')  %>
      </td>
      
    </tr>
  <% end %>
</table>
<%= generate_paginate.html_safe %>