<h1>新建版本</h1>
<%= form_for(@version, :url => "/admin_versions/create", :html =>{:multipart => true}) do |f| %>
  <p>
    版本:<br />
    <%= f.text_field :id %>
  </p>

  <p>
    功能描述:<br />
    <%= f.text_area :desc %>
  </p>

  <p>
    强制升级:
    <%= f.check_box :upgrade %>
  </p>

  <p>
    上传文件:
    <%= file_field_tag :file %>
  </p>

  <p>
    <%= f.submit "提交"%>
  </p>
<%  end %>