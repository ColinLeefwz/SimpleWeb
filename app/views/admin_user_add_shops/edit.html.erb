<%= form_for(@shop, :url => "/admin_user_add_shops/update?id=#{@shop.id}") do |f| %>
  <p>
    名称:<br />
    <%= f.text_field :name %>
  </p>
  <p>
    地址:<br />
    <%= f.text_field :addr %>
  </p>
  <p>
    实际经纬度:<br />
    <%= f.text_field :lob %>(例:30.28, 120.108)
  </p>

  <p>
    电话:<br />
    <%= f.text_field :tel %>
  </p>
  <p>
    类型:<br />
    <%= f.select :t,options_for_select(import_t)  %>
  </p>


  <p>
    <%= f.submit "保存" %>
  </p>
<% end %>

