<%= form_for(@shop, :url => "/admin_user_add_shops/update?id=#{@shop.id}") do |f| %>
  <p>
    名称:<br />
    <%= f.text_field :name %>
  </p>
  <p>
    地址:<br />
    <%= f.text_field :addr %>
  </p>
  <p>
    百度纬度度:<br />
    <%= f.text_field :lob %>(例:120.108,30.28 )
  </p>
  <p>
    类型:<br />
    <%= f.select :t,options_for_select(import_t)  %>
  </p>
  <p>
    <%= f.submit "保存" %>
  </p>
<% end %>

